refreshBtn.Name = "Refresh"
refreshBtn.Size = UDim2.new(0, 100, 0, 28)
refreshBtn.Position = UDim2.new(0, 8, 0, 40)
refreshBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
