task.spawn(function()
    while wait(0.07) do
        local s = Instance.new("Frame", homePage)
        s.Size = UDim2.new(0,4,0,4)
        s.Position = UDim2.new(math.random(),0,0,-10)
        s.BackgroundColor3 = Color3.new(1,1,1)
        s.ZIndex = 10
        Instance.new("UICorner", s).CornerRadius = UDim.new(1,0)
        TweenService:Create(s, TweenInfo.new(4), {
            Position = UDim2.new(math.random(),0,1,10),
            BackgroundTransparency = 1
        }):Play()
        game.Debris:AddItem(s, 4.1)
    end
end)

