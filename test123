    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(1, 0, 0, 20) -- Reduced height
    btn.Text = gun
    btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 10
    btn.Parent = scrollingFrame
