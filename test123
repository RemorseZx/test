toggle.Size = UDim2.new(1, -20, 0, 35)
toggle.Position = UDim2.new(0, 10, 0, 230)
toggle.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
toggle.Text = "Start Auto Give"
