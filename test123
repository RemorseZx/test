watermark.Font = Enum.Font.GothamBold
watermark.TextSize = 16
Instance.new("UICorner", watermark).CornerRadius = UDim.new(0, 8)

task.spawn(function()
    while wait(3) do
        for i = 1, 40 do
            watermark.Text = ("Monsy V3 / discord.gg/3ktg / mwamwa"):sub(1, i)
            wait(0.05)
        end
        wait(2)
    end
end)

-- NAV BAR
