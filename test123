-- WATERMARK
local watermark = Instance.new("TextLabel", gui)
watermark.Size = UDim2.new(0, 500, 0, 30)
watermark.Position = UDim2.new(0.5, -250, 0, 8)
watermark.BackgroundColor3 = Color3.new(0,0,0)
watermark.BackgroundTransparency = 0.4
watermark.Text = ""
