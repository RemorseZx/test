		local rand = others[math.random(1, #others)]
		carryRemote:FireServer({
			cmd = "Carry",
			carrychoicesss = carryChoice,
			firstPlr = rand
		})
		wait(0.02)
		carryRemote:FireServer({ cmd = "Declinecarry" })
	end
