-- Stop Button (White)
local stopBtn = Instance.new("TextButton")
stopBtn.Text = "STOP"
stopBtn.Size = UDim2.new(0, 120, 0, 30)
stopBtn.Position = UDim2.new(0, 155, 0, 125)
