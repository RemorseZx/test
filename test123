-- SETTINGS (FULL)
-- ===================================
local setPage = pages["SETTINGS"]

local transparencyLabel = Instance.new("TextLabel", setPage)
transparencyLabel.Size = UDim2.new(0, 300, 0, 30)
transparencyLabel.Position = UDim2.new(0, 50, 0, 30)
transparencyLabel.BackgroundTransparency = 1
transparencyLabel.Text = "Transparency: 0%"
transparencyLabel.TextColor3 = Color3.new(1,1,1)
transparencyLabel.Font = Enum.Font.Gotham

local sliderTrack = Instance.new("Frame", setPage)
sliderTrack.Size = UDim2.new(0, 400, 0, 12)
sliderTrack.Position = UDim2.new(0, 50, 0, 65)
sliderTrack.BackgroundColor3 = Color3.fromRGB(30,30,30)
Instance.new("UICorner", sliderTrack).CornerRadius = UDim.new(0, 6)

local sliderKnob = Instance.new("TextButton", sliderTrack)
sliderKnob.Size = UDim2.new(0, 24, 0, 24)
sliderKnob.Position = UDim2.new(0, 0, 0.5, -12)
sliderKnob.BackgroundColor3 = Color3.new(1,1,1)
sliderKnob.Text = ""
Instance.new("UICorner", sliderKnob).CornerRadius = UDim.new(1, 0)

local draggingSlider = false
local function setTransparency(pct)
    pct = math.clamp(pct, 0, 1)
    main.BackgroundTransparency = pct * 0.8
    glow.Transparency = 0.5 + pct * 0.5
    transparencyLabel.Text = "Transparency: " .. math.floor(pct*100) .. "%"
end
setTransparency(0)

sliderKnob.MouseButton1Down:Connect(function() draggingSlider = true end)
UserInputService.InputEnded:Connect(function(i)
    if i.UserInputType == Enum.UserInputType.MouseButton1 then draggingSlider = false end
end)
RunService.RenderStepped:Connect(function()
    if draggingSlider then
        local x = UserInputService:GetMouseLocation().X - sliderTrack.AbsolutePosition.X
        local pct = math.clamp(x / sliderTrack.AbsoluteSize.X, 0, 1)
        sliderKnob.Position = UDim2.new(pct, -12, 0.5, -12)
        setTransparency(pct)
    end
end)

-- KEYBIND
local keyLabel = Instance.new("TextLabel", setPage)
keyLabel.Size = UDim2.new(0, 300, 0, 30)
keyLabel.Position = UDim2.new(0, 50, 0, 110)
keyLabel.BackgroundTransparency = 1
keyLabel.Text = "Toggle Key: K"
keyLabel.TextColor3 = Color3.new(1,1,1)
keyLabel.Font = Enum.Font.Gotham

local KeybindBtn = Instance.new("TextButton", setPage)
KeybindBtn.Size = UDim2.new(0, 160, 0, 40)
KeybindBtn.Position = UDim2.new(0, 50, 0, 145)
KeybindBtn.BackgroundColor3 = Color3.new(0,0,0)
KeybindBtn.Text = "CHANGE KEY"
KeybindBtn.TextColor3 = Color3.new(1,1,1)
KeybindBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", KeybindBtn).CornerRadius = UDim.new(0,10)
Instance.new("UIStroke", KeybindBtn).Color = Color3.new(1,1,1)

local capturingKey = false
local toggleKey = Enum.KeyCode.K
KeybindBtn.MouseButton1Click:Connect(function()
    capturingKey = true
    KeybindBtn.Text = "PRESS KEY..."
end)
UserInputService.InputBegan:Connect(function(inp)
    if capturingKey and inp.KeyCode ~= Enum.KeyCode.Unknown then
        toggleKey = inp.KeyCode
        keyLabel.Text = "Toggle Key: " .. inp.KeyCode.Name
        KeybindBtn.Text = "CHANGE KEY"
        capturingKey = false
    end
end)

-- THEME
local themeLabel = Instance.new("TextLabel", setPage)
themeLabel.Size = UDim2.new(0, 300, 0, 40)
themeLabel.Position = UDim2.new(0, 50, 0, 200)
themeLabel.BackgroundTransparency = 1
themeLabel.Text = "UI Theme"
themeLabel.TextColor3 = Color3.new(1,1,1)
themeLabel.Font = Enum.Font.GothamBold

local palette = Instance.new("Frame", setPage)
palette.Size = UDim2.new(0, 380, 0, 40)
palette.Position = UDim2.new(0, 50, 0, 240)
palette.BackgroundTransparency = 1

local themes = {
    {name="PURE", bg=Color3.new(0,0,0), glow=Color3.new(1,1,1)},
    {name="VOID", bg=Color3.new(0,0,0), glow=Color3.fromRGB(0,255,255)},
    {name="BLOOD", bg=Color3.new(0,0,0), glow=Color3.fromRGB(255,0,0)},
}
for i, t in ipairs(themes) do
    local sw = Instance.new("TextButton", palette)
    sw.Size = UDim2.new(0, 110, 0, 36)
    sw.Position = UDim2.new(0, (i-1)*120, 0, 0)
    sw.BackgroundColor3 = t.bg
    sw.Text = t.name
    sw.TextColor3 = Color3.new(1,1,1)
    sw.Font = Enum.Font.GothamBold
    Instance.new("UICorner", sw).CornerRadius = UDim.new(0,8)
    local stroke = Instance.new("UIStroke", sw)
    stroke.Color = t.glow
    sw.MouseButton1Click:Connect(function()
        main.BackgroundColor3 = t.bg
        glow.Color = t.glow
        themeLabel.Text = "UI Theme: " .. t.name
    end)
end

-- WATERMARK TOGGLE
local wmBtn = Instance.new("TextButton", setPage)
wmBtn.Size = UDim2.new(0, 80, 0, 40)
wmBtn.Position = UDim2.new(0, 50, 0, 300)
wmBtn.BackgroundColor3 = Color3.fromRGB(0,220,0)
wmBtn.Text = "ON"
wmBtn.TextColor3 = Color3.new(0,0,0)
wmBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", wmBtn).CornerRadius = UDim.new(0, 12)

local wmLabel = Instance.new("TextLabel", setPage)
wmLabel.Size = UDim2.new(0, 200, 0, 40)
wmLabel.Position = UDim2.new(0, 140, 0, 300)
wmLabel.BackgroundTransparency = 1
wmLabel.Text = "Watermark"
wmLabel.TextColor3 = Color3.new(1,1,1)
wmLabel.Font = Enum.Font.Gotham

wmBtn.MouseButton1Click:Connect(function()
    watermark.Visible = not watermark.Visible
    wmBtn.Text = watermark.Visible and "ON" or "OFF"
    wmBtn.BackgroundColor3 = watermark.Visible and Color3.fromRGB(0,220,0) or Color3.fromRGB(220,0,0)
end)

-- ===================================
-- EXECUTOR (FULL)
-- ===================================
local execPage = pages["EXECUTOR"]
local ExecTitle = Instance.new("TextLabel", execPage)
ExecTitle.Size = UDim2.new(1, -40, 0, 40)
ExecTitle.Position = UDim2.new(0, 20, 0, 10)
ExecTitle.BackgroundTransparency = 1
ExecTitle.Text = "MONSY EXECUTOR"
ExecTitle.TextColor3 = Color3.new(1,1,1)
ExecTitle.Font = Enum.Font.GothamBlack
ExecTitle.TextSize = 24

local ScriptBox = Instance.new("TextBox", execPage)
ScriptBox.Size = UDim2.new(1, -40, 1, -140)
ScriptBox.Position = UDim2.new(0, 20, 0, 60)
ScriptBox.BackgroundColor3 = Color3.new(0,0,0)
ScriptBox.Text = "-- Lua Script"
ScriptBox.TextColor3 = Color3.new(1, 1, 1) 
ScriptBox.ClearTextOnFocus = false
ScriptBox.MultiLine = true
ScriptBox.TextXAlignment = Enum.TextXAlignment.Left
ScriptBox.TextYAlignment = Enum.TextYAlignment.Top
ScriptBox.Font = Enum.Font.Code
ScriptBox.TextSize = 14
Instance.new("UICorner", ScriptBox).CornerRadius = UDim.new(0,12)
Instance.new("UIStroke", ScriptBox).Color = Color3.new(1,1,1)

local OutputLabel = Instance.new("TextLabel", execPage)
OutputLabel.Size = UDim2.new(1, -40, 0, 50)
OutputLabel.Position = UDim2.new(0, 20, 1, -80)
OutputLabel.BackgroundColor3 = Color3.new(0,0,0)
OutputLabel.Text = "Output: Ready"
OutputLabel.TextColor3 = Color3.fromRGB(0,255,0)
OutputLabel.Font = Enum.Font.Gotham
OutputLabel.TextSize = 14
OutputLabel.TextWrapped = true
Instance.new("UICorner", OutputLabel).CornerRadius = UDim.new(0,8)
Instance.new("UIStroke", OutputLabel).Color = Color3.fromRGB(0,255,0)

local ExecBtn = Instance.new("TextButton", execPage)
ExecBtn.Size = UDim2.new(0, 140, 0, 50)
ExecBtn.Position = UDim2.new(0, 20, 1, -70)
ExecBtn.BackgroundColor3 = Color3.new(0,0,0)
ExecBtn.Text = "EXECUTE"
ExecBtn.TextColor3 = Color3.new(1,1,1)
ExecBtn.Font = Enum.Font.GothamBlack
ExecBtn.TextSize = 16
Instance.new("UICorner", ExecBtn).CornerRadius = UDim.new(0,12)
Instance.new("UIStroke", ExecBtn).Color = Color3.new(1,1,1)

local ClearBtn = Instance.new("TextButton", execPage)
ClearBtn.Size = UDim2.new(0, 140, 0, 50)
ClearBtn.Position = UDim2.new(0, 170, 1, -70)
ClearBtn.BackgroundColor3 = Color3.new(0,0,0)
ClearBtn.Text = "CLEAR"
ClearBtn.TextColor3 = Color3.new(1,1,1)
ClearBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", ClearBtn).CornerRadius = UDim.new(0,12)
Instance.new("UIStroke", ClearBtn).Color = Color3.new(1,1,1)

local CopyBtn = Instance.new("TextButton", execPage)
CopyBtn.Size = UDim2.new(0, 140, 0, 50)
CopyBtn.Position = UDim2.new(0, 280, 1, -70)
CopyBtn.BackgroundColor3 = Color3.new(0,0,0)
CopyBtn.Text = "COPY"
CopyBtn.TextColor3 = Color3.new(1,1,1)
CopyBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", CopyBtn).CornerRadius = UDim.new(0,12)
Instance.new("UIStroke", CopyBtn).Color = Color3.fromRGB(0,200,255)

ExecBtn.MouseButton1Click:Connect(function()
    local code = ScriptBox.Text
    if #code < 5 then OutputLabel.Text = "Output: Empty!" return end
    OutputLabel.Text = "Output: Running..."
    task.spawn(function()
        local f, err = loadstring(code)
        if not f then OutputLabel.Text = "Output: ERROR - "..err return end
        local ok, res = pcall(f)
        OutputLabel.Text = ok and "Output: Success!" or "Output: ERROR - "..tostring(res)
    end)
end)
ClearBtn.MouseButton1Click:Connect(function()
    ScriptBox.Text = ""
    OutputLabel.Text = "Output: Cleared"
end)
CopyBtn.MouseButton1Click:Connect(function()
    setclipboard(ScriptBox.Text)
    OutputLabel.Text = "Output: Copied!"
end)

-- WORLD PAGE
local worldPage = pages["UNIVERSAL"]
local worldLbl = Instance.new("TextLabel", worldPage)
worldLbl.Size = UDim2.new(1,0,1,0)
worldLbl.BackgroundTransparency = 1
worldLbl.Text = "UNIVERSAL SCRIPT\nComing Soon..."
worldLbl.TextColor3 = Color3.new(1,1,1)
worldLbl.Font = Enum.Font.GothamBold
worldLbl.TextSize = 50

-- TOGGLE + FLY
UserInputService.InputBegan:Connect(function(i, gp)
    if gp then return end
    if i.KeyCode == toggleKey then
        gui.Enabled = not gui.Enabled
    elseif i.KeyCode == Enum.KeyCode.F then
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local bv = Instance.new("BodyVelocity")
            bv.Velocity = Vector3.new(0,60,0)
            bv.MaxForce = Vector3.new(5000,5000,5000)
            bv.Parent = LocalPlayer.Character.HumanoidRootPart
            task.wait(2.5)
            bv:Destroy()
        end
    end
end)

-- MAIN SNOW
task.spawn(function()
    while wait(0.07) do
        local s = Instance.new("Frame", main)
        s.Size = UDim2.new(0,3,0,3)
        s.Position = UDim2.new(math.random(),0,0,-10)
        s.BackgroundColor3 = Color3.new(1,1,1)
        s.ZIndex = 10
        Instance.new("UICorner", s).CornerRadius = UDim.new(1,0)
        TweenService:Create(s, TweenInfo.new(4), {Position = UDim2.new(math.random(),0,1,10), BackgroundTransparency = 1}):Play()
        game.Debris:AddItem(s, 4.1)
    end
end)

-- START
pages["HOME"].Visible = true
buttons[1].BackgroundColor3 = Color3.fromRGB(40,40,40)
refresh()



