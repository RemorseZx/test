left.BackgroundColor3 = Color3.new(0,0,0)
left.ScrollBarThickness = 5
left.AutomaticCanvasSize = Enum.AutomaticSize.Y
Instance.new("UICorner", left).CornerRadius = UDim.new(0, 12)

local search = Instance.new("TextBox", left)
search.Size = UDim2.new(1, -20, 0, 35)
search.Position = UDim2.new(0, 10, 0, 10)
search.BackgroundColor3 = Color3.new(0,0,0)
search.PlaceholderText = "Search player..."
search.PlaceholderColor3 = Color3.fromRGB(100,100,100)
search.TextColor3 = Color3.new(1,1,1)
search.Font = Enum.Font.Gotham
Instance.new("UICorner", search).CornerRadius = UDim.new(0, 8)

