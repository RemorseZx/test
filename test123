dropdown.Parent = frame
Instance.new("UICorner", dropdown).CornerRadius = UDim.new(0, 6)

local gunList = {"sophia", "daniel", "cuervo", "anghelgirls", "coach", "berto", "jeric", "killua", "alaspogi", "xynu", "chris", "astig", "sup", "licht", "devil", "UZI"}
