    btn.MouseButton1Click:Connect(function()
        selectedGun = gun
        dropdown.Text = gun
        scrollingFrame.Visible = false
    end)
end

dropdown.MouseButton1Click:Connect(function()
    scrollingFrame.Visible = not scrollingFrame.Visible
end)

