right.Size = UDim2.new(0, 560, 1, 0)
right.Position = UDim2.new(0, 320, 0, 0)
right.BackgroundColor3 = Color3.new(0,0,0)
Instance.new("UICorner", right).CornerRadius = UDim.new(0, 12)

local selLabel = Instance.new("TextLabel", right)
selLabel.Size = UDim2.new(1, -20, 0, 40)
selLabel.Position = UDim2.new(0, 10, 0, 10)
selLabel.BackgroundTransparency = 1
selLabel.Text = "Select a player"
selLabel.TextColor3 = Color3.new(1,1,1)
selLabel.Font = Enum.Font.GothamBold
selLabel.TextSize = 20

local actions = {"View Player", "Unview", "Teleport To", "Copy Outfit", "Bring Player"}
local actionBtns = {}
for i, txt in ipairs(actions) do
    local b = Instance.new("TextButton")
    b.Size = UDim2.new(0, 240, 0, 45)
    b.Position = UDim2.new(0, 20 + (i>3 and 260 or 0), 0, 60 + ((i-1)%3)*55)
    b.BackgroundColor3 = Color3.new(0,0,0)
    b.Text = txt
    b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamBold
    b.Parent = right
    Instance.new("UICorner", b).CornerRadius = UDim.new(0, 10)
    local stroke = Instance.new("UIStroke", b)
    stroke.Color = Color3.new(1,1,1)
    stroke.Transparency = 0.8
    actionBtns[txt] = b
end

local selected = nil
local function refresh()
    for _, c in ipairs(list:GetChildren()) do if c:IsA("TextButton") then c:Destroy() end end
    for _, plr in ipairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then
            local btn = Instance.new("TextButton")
            btn.Size = UDim2.new(1, 0, 0, 38)
            btn.BackgroundColor3 = Color3.new(0,0,0)
            btn.Text = plr.DisplayName.." (@"..plr.Name..")"
            btn.TextColor3 = Color3.new(1,1,1)
            btn.Font = Enum.Font.Gotham
            btn.Parent = list
            Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 8)
            local stroke = Instance.new("UIStroke", btn)
            stroke.Color = Color3.new(1,1,1)
            stroke.Transparency = 0.9
            btn.MouseButton1Click:Connect(function()
                selected = plr
                selLabel.Text = "Selected: "..plr.DisplayName
            end)
        end
    end
end

search.FocusLost:Connect(function(enter)
    if enter then
        local q = search.Text:lower()
        for _, b in ipairs(list:GetChildren()) do
            if b:IsA("TextButton") then
                b.Visible = b.Text:lower():find(q) ~= nil
            end
        end
    end
end)

actionBtns["View Player"].MouseButton1Click:Connect(function()
    if selected and selected.Character then
        workspace.CurrentCamera.CameraSubject = selected.Character.Humanoid
    end
end)
actionBtns["Unview"].MouseButton1Click:Connect(function()
    workspace.CurrentCamera.CameraSubject = LocalPlayer.Character.Humanoid
end)
actionBtns["Teleport To"].MouseButton1Click:Connect(function()
    if selected and LocalPlayer.Character then
        LocalPlayer.Character:MoveTo(selected.Character.HumanoidRootPart.Position)
    end
end)
actionBtns["Copy Outfit"].MouseButton1Click:Connect(function()
    if selected then
        LocalPlayer.Character:Destroy()
        selected.Character:Clone().Parent = workspace
        task.wait(0.5)
        LocalPlayer.Character = workspace[LocalPlayer.Name]
    end
end)
actionBtns["Bring Player"].MouseButton1Click:Connect(function()
    if selected and LocalPlayer.Character then
        selected.Character:MoveTo(LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(4,0,0))
    end
end)

refresh()
Players.PlayerAdded:Connect(refresh)
Players.PlayerRemoving:Connect(refresh)

